{
  "manifest_version": 2,
  "version": "1.0.0",
  "name": "SIKS tools",
  "description": "Aplikasi untuk optimasi Sistem Informasi Kesejahteraan Sosial",
  "offline_enabled": true,
  "icons": {
    "16": "data/icons/icon.png",
    "19": "data/icons/icon.png",
    "32": "data/icons/icon.png",
    "38": "data/icons/icon.png",
    "48": "data/icons/icon.png",
    "64": "data/icons/icon.png",
    "128": "data/icons/icon.png",
    "256": "data/icons/icon.png"
  },
  "permissions": [
    "storage",
    "contextMenus",
    "notifications"
  ],
  "optional_permissions": [
    "*://*/*"
  ],
  "sandbox": {
    "pages": ["data/manager-old/sandbox.html"]
  },
  "browser_action": {},
  "background": {
    "persistent": false,
    "scripts": [
      "config.js",
      "js/jquery.min.js",
      "js/background/promise.js",
      "js/background/bg-functions.js",
      "js/background/background.js",
      "background.js"
    ]
  }
}
